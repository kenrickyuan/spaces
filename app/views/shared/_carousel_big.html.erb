<% number_of_times = test.count - 1 %>
<div id="carouselExampleIndicatorsBig" class="carousel slide" data-ride="carousel" data-interval="false">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicatorsBig" data-slide-to="0" class="active"></li>
    <% number_of_times.times do |n| %>
    <li data-target="#carouselExampleIndicatorsBig" data-slide-to=n></li>
    <% end %>
  </ol>
  <div class="carousel-inner-big carousel-inner">
    <div class="carousel-item active">
      <%= cl_image_tag test[0].photo, class: "d-block carousel-image-big" %>
    </div>
    <% number_of_times.times do |n| %>
    <div class="carousel-item">
      <%= cl_image_tag test[n + 1].photo, class: "d-block carousel-image-big" %>
    </div>
    <% end %>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleIndicatorsBig" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleIndicatorsBig" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
